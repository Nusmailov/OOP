package Problem5;

public abstract class Knight extends Piece{
	Knight(){
		Position a = null;
		Position b = null;
		boolean bl= false;
		if (super.check(a, b)) {
			System.out.println("Error");
		}
		else{
			return (((Math.abs(a.x-b.x) == 1) && (Math.abs(a.y-b.y) == 2)) || ((Math.abs(a.x-b.x)==2) && (Math.abs(a.y-b.y)==1)));
		}
		
	}
		
}
