package InLab;

public class Example1
{
    public static void main(String[] args){
		int denominator, numerator, ratio;
	
		numerator   = 5;
		denominator = 0;
	
	    ratio = numerator / denominator;
	    try {
	    	System.out.println("The answer is: " + ratio);
	    }
	    catch{
	    	System.out.println("NotDone."); // Don't move this line
	    }
    }	
}

